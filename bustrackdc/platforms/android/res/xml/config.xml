<?xml version='1.0' encoding='utf-8'?>
<widget id="com.JasonRosenbaum.BusTrackDC" version="1.8.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="MapKit">
        <param name="android-package" value="com.phonegap.plugins.mapkit.MapKit" />
    </feature>
    <name>BusTrackDC</name>
    <description>
        A bus tracking app for the Washington, DC metro rail and bus and circulator system.
    </description>
    <author email="seminal@theseminal.com" href="http://bustrackdc.com">
        Jason Rosenbaum
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
